{
  "name": "bagetaexpress",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ts:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:push": "drizzle-kit push:sqlite --config=drizzle.config.ts",
    "migration:generate": "drizzle-kit generate:sqlite --schema=./db/schema.ts",
    "migration:push": "node -r esbuild-register db/migrate.ts",
    "migration:pushc": "node -r esbuild-register db/migrate.ts --clear",
    "migration:pushp": "node -r esbuild-register db/migrate.ts --populate",
    "migration:pushcp": "node -r esbuild-register db/migrate.ts --clear --populate",
    "migration:studio": "drizzle-kit studio; drizzle-kit studio --port 3000; drizzle-kit studio --verbose "
  },
  "dependencies": {
